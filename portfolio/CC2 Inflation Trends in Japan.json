{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Inflation Trends-Japan",
    "anchor": "start",
    "subtitle": "CPI, % | Source: JSB (WB) through ECO API",
    "fontSize": "14",
    "subtitleFontSize": "10",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": "300",
  "width": "430",
  "autosize": {"type": "fit", "contains": "padding", "resize": false},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/jpn/infl?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "line", "color": "#2016b1", "strokeWidth": "2"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  }
}